{
  "name": "catbox-memcached2",
  "version": "3.0.1",
  "description": "Memcached adapter for catbox. Supports events, more than 30 days ttl and ready to use for production",
  "keywords": [
    "memcached",
    "adapter",
    "catbox",
    "cache",
    "hapi",
    "hapijs"
  ],
  "homepage": "http://github.com/cmfatih/catbox-memcached2",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmfatih/catbox-memcached2.git"
  },
  "bugs": {
    "url": "http://github.com/cmfatih/catbox-memcached2/issues"
  },
  "license": "MIT",
  "private": false,
  "author": {
    "name": "cmfatih",
    "url": "http://github.com/cmfatih"
  },
  "contributors": [],
  "main": "./index.js",
  "scripts": {
    "lint": "jshint --reporter node_modules/jshint-stylish lib/* test/*",
    "lint:build": "jshint --reporter checkstyle lib/* test/* > reports/jshint-checkstyle.xml",
    "depcheck": "dependency-check . lib/*",
    "depcheck:unused": "dependency-check ./package.json --unused --no-dev lib/*",
    "test:unit": "mkdir -p reports/ && NODE_ENV=test multi='spec=- xunit=reports/mocha-xunit.xml' istanbul cover _mocha -- -R mocha-multi --timeout 10000 && istanbul check-coverage",
    "test": "npm run depcheck && npm run depcheck:unused && npm run lint && npm run test:unit"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "memcached": "2.1.x"
  },
  "devDependencies": {
    "chai": "3.2.x",
    "jshint": "2.8.x",
    "jshint-stylish": "2.0.x",
    "mocha": "2.2.x",
    "mocha-multi": "0.7.x",
    "dependency-check": "2.5.x",
    "istanbul": "^0.3.17",
    "catbox": "4.x.x"
  }
}